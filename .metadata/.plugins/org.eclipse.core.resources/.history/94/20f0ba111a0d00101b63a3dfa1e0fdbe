package Ejercicio1;

public class SimpleLinkedList<T> {

	Node<T> head;
	int size;

	public class Node<T> {
		T data;
		Node<T> next;

		public Node(T data) {
			this.data = data;
			next = null;
		}

	}

	public SimpleLinkedList() {
		head = null;
		size = 0;
	}

	public void insertAtEnd(T data) {
		Node<T> newNode = new Node<T>(data);

		if (head == null) {
			head = newNode;
			return;
		} else {
			Node<T> last = head;
			while (last.next != null) {
				last = last.next;

			}
			last.next = newNode;
			size++;

		}

	}

	public int size() {
		return size;
	}

	public void printList() {
		Node<T> temp = head;
		while (temp != null) {
			System.out.println(temp.data + " ");

			temp = temp.next;
		}
		System.out.println();
	}

}
